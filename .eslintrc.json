{
  "extends": [
    "next/core-web-vitals",
    "prettier",
    "plugin:import/recommended",
    "plugin:import/warnings",
    "plugin:tailwindcss/recommended"
  ],
  "plugins": ["tailwindcss"],
  "rules": {
    "import/order": [
      "error",
      { "alphabetize": { "order": "asc", "caseInsensitive": true } }
    ]
  }
}
